{
  "extends": ["eslint:recommended", "prettier"],
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    // "strict": ["error", "global"],
    // "import/no-extraneous-dependencies": ["error", {"devDependencies": false, "optionalDependencies": false, "peerDependencies": false}]
    "no-console": "off",
    "curly": "warn",
    "no-unused-vars": "warn"
  }
}